{
    "name": "rebelcode/wp-settings",
    "description": "An object-oriented API for WordPress settings.",
    "type": "library",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "RebelCode",
            "email": "dev@rebelcode.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.4 | ^7.0",
        "dhii/data-key-value-aware-trait": "dev-task/initial-traits",
        "rebelcode/wp-settings-interface": "dev-task/initial-interfaces",
        "rebelcode/wp-nonce-interface": "dev-task/initial-interfaces",
        "dhii/validation-base": "^0.1.0",
        "dhii/data-container-interface": "dev-release/0.1",
        "dhii/i18n-helper-base": "dev-develop",
        "dhii/i18n-interface": "^0.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "ptrofimov/xpmock": "^1.1",
        "dhii/php-cs-fixer-config": "^0.1",
        "codeclimate/php-test-reporter": "<=0.3.2",
        "dhii/stringable-interface": "^0.1"
    },
    "autoload": {
        "psr-4": {
            "RebelCode\\WordPress\\Admin\\Settings\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit",
        "csfix": "php-cs-fixer fix -vvv"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/RebelCode/wp-settings-interface"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/output-renderer-interface"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/human-readable-interface"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/data-key-value-aware-trait"
        },
        {
            "type": "vcs",
            "url": "https://github.com/RebelCode/wp-nonce-interface"
        }
    ]
}
